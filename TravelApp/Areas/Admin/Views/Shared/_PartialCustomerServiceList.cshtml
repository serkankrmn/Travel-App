@using TravelApp.Areas.Admin.Models

@model CustomerServiceListPageModel


@if (Model.ServiceList == null || Model.ServiceList.Count == 0)
{
    <tr>
        <td colspan="9">
            <div style="text-align:center">
                Veri Bulunamadı
            </div>

        </td>
    </tr>
}
else
{


    foreach (var item in Model.ServiceList)
    {


        @await Html.PartialAsync("_PartialCustomerServiceTaskItem", item)

    }
}
